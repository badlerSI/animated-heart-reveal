{
  "redirects": [
    {
      "source": "https://www.soulinterface.ai/(.*)",
      "destination": "https://soulinterface.ai/$1",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/(.*)\\.(?:png|jpg|jpeg|gif|webp|svg)$",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
